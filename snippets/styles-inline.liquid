<style rel="stylesheet">
{%- render 'preload-styles.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- if template.name == 'index' -%}
	{%- render 'preload-home-page.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- else -%}
	{%- render 'preload-breadcrumbs.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if settings.promo_enable -%}
{%- unless settings.enable_rtl and settings.rtl_language_codes contains request.locale.iso_code -%}
.promo__item:last-child {padding-right: 0;}
{%- else -%}
.promo__item:first-child {padding-right: 0;}
{%- endunless -%}
{%- render 'preload-promotion.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'product' -%}
	{%- render 'preload-product-single-general.scss' with left_holder: left_holder, right_holder: right_holder -%}
  {%- render 'preload-product-single.scss' with left_holder: left_holder, right_holder: right_holder -%}
	{%- render 'preload-product-single-fre-bought.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'collection' or template.name == 'search' -%}
  {%- render 'preload-collection-page.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'search' -%}
  {%- render 'preload-search-page.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'blog' or template.name == 'article' -%}
  {%- render 'preload-blog-page.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'cart' -%}
  {%- render 'preload-cart-page.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if settings.header_style == 'style6' -%}{%- render 'preload-header-support-area.scss' with left_holder: left_holder, right_holder: right_holder -%}{%- endif -%}
{%- if settings.lazyload_effect -%}
  {%- render 'preload-lazy-load.scss' -%}
{%- endif -%}
{%- if settings.thumbnail_ratio != 'auto' -%}.pg__image{padding-top:{{ settings.thumbnail_ratio }}%;}.pg__img-fit{-o-object-fit:contain;object-fit:contain;height:100%;}{%- endif -%}
</style>
<style rel="stylesheet" media="all and (min-width:992px)">
{%- case settings.header_style -%}
  {%- when 'style1' -%}
    {%- render 'preload-header-style1.scss' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style2' -%}
    {%- render 'preload-header-style2.scss' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style3' -%}
    {%- render 'preload-header-style3.scss' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style4' -%}
    {%- render 'preload-header-style4.scss' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style5' -%}
    {%- render 'preload-header-style5.scss' with left_holder: left_holder, right_holder: right_holder -%}
  {%- else -%}
    {%- render 'preload-header-style6.scss' with left_holder: left_holder, right_holder: right_holder -%}
{%- endcase -%}
.aligning-menu{overflow-x:hidden;}
</style>